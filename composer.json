{
    "name": "esslassi/metable",
    "description": "Laravel MetaData for Eloquent Models",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Esslassi\\Metable\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Esslassi Mohammed",
            "email": "esslassi1996@gmail.com",
            "homepage": "https://github.com/esslassi",
